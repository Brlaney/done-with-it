{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 14",
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "commonjs",
    "target": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src",
    "**/*.ts",
    "**/*.tsx"
  ]
}
